{
  "name": "sistema-tirar-medidas-roupas",
  "version": "1.0.0",
  "description": "Um sistema para tirar medidas de roupas",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "jest",
  "db:create": "sequelize-cli db:create",
  "db:drop": "sequelize-cli db:drop",
  "db:migrate": "sequelize-cli db:migrate",
  "db:seed:all": "sequelize-cli db:seed:all",
  "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "sistema",
    "tirar medidas",
    "roupas"
  ],
  "author": "Ramilton",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^2.0.14",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  }
}
